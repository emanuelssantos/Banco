package Classes;



public class Conta{
    private String usuario;
    private String senha;
    private double saldo;


    public Conta(String usuario, String senha, double saldo){
        this.usuario = usuario;
        this.senha = senha;
    }

    public transferir(Conta user,double valor){
        if(valor <= this.saldo){
            this.saldo -= valor;
            user.saldo += valor;
        }
        return;
    }




}